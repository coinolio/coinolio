  "license": "MIT",
  "bugs": "https://github.com/coinolio/coinolio/issues",
  "contributors": [
    "https://github.com/coinolio/coinolio/graphs/contributors"
  ],
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "eslint": "^4.14.0",
    "eslint-config-google": "^0.9.1",
    "jest": "^22.0.4",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-redis": "^3.3.3",
