    "lint": "eslint --ext .js core",
    "test": "npm run lint && npm run unit"
  },
  "engines": {
    "node": "^6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coinolio/coinolio.git"
  },
  "keywords": [
    "coinolio",
